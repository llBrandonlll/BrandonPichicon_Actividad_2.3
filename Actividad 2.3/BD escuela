CREATE TABLE Matriculas (
ID_matricula INT AUTO_INCREMENT,
RUT VARCHAR(12),
ID_Curso INT,
Fecha_Matricula DATE NOT NULL,
PRIMARY KEY (ID_matricula),
FOREIGN KEY (RUT) REFERENCES Estudiantes(RUT),
FOREIGN KEY (ID_Curso) REFERENCES Cursos(ID_Curso));


